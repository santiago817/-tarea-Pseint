Algoritmo sin_titulo
	Dimension carga[6]
	Definir reducido Como Entero
	Definir pocicion Como Entero
	reducido = 99999999
	
	Escribir "ingrese carga de vectores"
    para i= 0 hasta 5 Con Paso 1 Hacer
		Leer carga[i]
	FinPara
	
	para i=0 Hasta 5 con paso 1 Hacer
		si (carga[i]<reducido)Entonces
			reducido=carga[i]
			pocicion=i
		FinSi
		
	FinPara
	
	
	
	Escribir "el vector 0 tiene ",carga[0]
	Escribir "el vector 1 tiene ",carga[1]
	Escribir "el vector 2 tiene ",carga[2]
	Escribir "el vector 3 tiene ",carga[3]
	Escribir "el vector 4 tiene ",carga[4]
	Escribir "el vector 5 tiene ",carga[5]
	
	
	Escribir "la carga menor de los vectores es de  ", reducido " y ","esta en la pocicion ",pocicion
	
	
	
	
FinAlgoritmo
